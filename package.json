{
  "name": "rainbow-rims-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "baseline": "tsc -p tsconfig.baseline.json && node baseline-dist/scripts/runBaseline.js",
    "baseline:check": "tsc -p tsconfig.baseline.json && node baseline-dist/scripts/checkBaseline.js",
    "cross-tier:check": "tsc -p tsconfig.baseline.json && node baseline-dist/scripts/runCrossTierValidation.js",
    "cross-tier:update": "tsc -p tsconfig.baseline.json && node baseline-dist/scripts/updateCrossTierBaseline.js",
    "test": "tsc -p tsconfig.tests.json && node --test \"build/tests/tests/**/*.js\""
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.3.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.12"
  }
}
